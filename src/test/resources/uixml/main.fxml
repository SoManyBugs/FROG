<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.image.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<BorderPane prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="muffinc.frog.test.userinterface.MainController">
   <top>
      <MenuBar fx:id="menuBar" BorderPane.alignment="CENTER">
        <menus>
          <Menu mnemonicParsing="false" text="File">
            <items>
              <MenuItem mnemonicParsing="false" text="Close" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Edit">
            <items>
              <MenuItem mnemonicParsing="false" text="Delete" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Help">
            <items>
              <MenuItem mnemonicParsing="false" text="About" />
            </items>
          </Menu>
        </menus>
      </MenuBar>
   </top>
   <center>
      <TabPane prefHeight="200.0" prefWidth="200.0" tabClosingPolicy="UNAVAILABLE" BorderPane.alignment="CENTER">
        <tabs>
          <Tab text="Photo">
               <content>
                  <SplitPane dividerPositions="0.5">
                     <items>
                        <VBox AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.topAnchor="0.0">
                           <children>
                              <TableView fx:id="photoTable" editable="true" prefHeight="2000.0">
                                <columns>
                                    <TableColumn fx:id="photoNameColumn" prefWidth="75.0" text="PhotoName" />
                                  <TableColumn fx:id="countColumn" prefWidth="75.0" text="People Count" />
                                </columns>
                                 <columnResizePolicy>
                                    <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                                 </columnResizePolicy>
                              </TableView>
                              <ToolBar AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" VBox.vgrow="ALWAYS">
                                <items>
                                  <Button fx:id="addFileButton" mnemonicParsing="false" onAction="#setAddFileButton" text="Add File" textAlignment="CENTER" />
                                    <Region />
                                    <Button fx:id="deleteSelectedPhotoButton" mnemonicParsing="false" text="Delete Selected" textAlignment="CENTER" />
                                    <Region />
                                    <Button fx:id="scanNDetectButton" mnemonicParsing="false" onAction="#setScanNDetectButton" text="Scan&amp;Detect" textAlignment="CENTER" />
                                </items>
                              </ToolBar>
                           </children>
                        </VBox>
                        <SplitPane dividerPositions="0.379182156133829" orientation="VERTICAL">
                          <items>
                              <HBox alignment="CENTER" SplitPane.resizableWithParent="false">
                                 <children>
                                    <StackPane fx:id="photoImageViewParent" HBox.hgrow="ALWAYS">
                                       <children>
                                          <ImageView fx:id="photoImageView" pickOnBounds="true" preserveRatio="true" StackPane.alignment="CENTER" />
                                       </children>
                                    </StackPane>
                                 </children>
                              </HBox>
                              <Accordion prefHeight="386.0" prefWidth="396.0">
                                <panes>
                                  <TitledPane animated="false" text="Photo Info">
                                    <content>
                                      <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                                             <children>
                                                <GridPane alignment="CENTER" gridLinesVisible="true" layoutX="14.0" layoutY="142.0" prefHeight="76.0" prefWidth="140.0" AnchorPane.rightAnchor="46.0">
                                                  <columnConstraints>
                                                    <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                                                    <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                                                  </columnConstraints>
                                                  <rowConstraints>
                                                    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                                                    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                                                    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                                                  </rowConstraints>
                                                   <children>
                                                      <Label text="File Name" />
                                                      <Label text="Label" GridPane.columnIndex="1" />
                                                      <Label text="People" GridPane.rowIndex="1" />
                                                      <Label text="Label" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                                                      <Label text="Selected People" GridPane.rowIndex="2" />
                                                      <ChoiceBox prefWidth="150.0" GridPane.columnIndex="1" GridPane.rowIndex="2" />
                                                   </children>
                                                   <padding>
                                                      <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                                                   </padding>
                                                </GridPane>
                                             </children>
                                          </AnchorPane>
                                    </content>
                                  </TitledPane>
                                  <TitledPane animated="false" text="Faces">
                                    <content>
                                      <AnchorPane>
                                             <children>
                                                <ToolBar prefHeight="40.0" prefWidth="200.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                                                  <items>
                                                      <ComboBox fx:id="facesCombo" promptText="Please Scan This Photo First!">
                                                         <padding>
                                                            <Insets bottom="2.0" left="2.0" right="2.0" top="2.0" />
                                                         </padding>
                                                      </ComboBox>
                                                    <Button fx:id="deleteFaceButton" mnemonicParsing="false" onAction="#setDeleteFaceButton" text="Delete" />
                                                  </items>
                                                </ToolBar>
                                                <ImageView fx:id="faceImageView" fitHeight="200.0" fitWidth="180.0" pickOnBounds="true" preserveRatio="true" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" />
                                             </children>
                                          </AnchorPane>
                                    </content>
                                  </TitledPane>
                                </panes>
                              </Accordion>
                          </items>
                        </SplitPane>
                     </items>
                  </SplitPane>
               </content>
          </Tab>
          <Tab text="People">
            <content>
              <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                     <children>
                        <TableView layoutX="14.0" layoutY="25.0" prefHeight="338.0" prefWidth="284.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.topAnchor="0.0">
                          <columns>
                              <TableColumn prefWidth="75.0" text="Photo" />
                            <TableColumn prefWidth="75.0" text="Name" />
                            <TableColumn prefWidth="75.0" text="Total Number" />
                          </columns>
                           <columnResizePolicy>
                              <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                           </columnResizePolicy>
                        </TableView>
                     </children>
                  </AnchorPane>
            </content>
          </Tab>
        </tabs>
      </TabPane>
   </center>
</BorderPane>
