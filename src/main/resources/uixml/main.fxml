<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.image.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<BorderPane prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="muffinc.frog.userinterface.MainController">
   <top>
      <MenuBar fx:id="menuBar" BorderPane.alignment="CENTER">
        <menus>
          <Menu mnemonicParsing="false" text="File">
            <items>
              <MenuItem mnemonicParsing="false" text="Close" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Edit">
            <items>
              <MenuItem mnemonicParsing="false" text="Delete" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Help">
            <items>
              <MenuItem mnemonicParsing="false" text="About" />
            </items>
          </Menu>
        </menus>
      </MenuBar>
   </top>
   <center>
      <TabPane prefHeight="200.0" prefWidth="200.0" tabClosingPolicy="UNAVAILABLE" BorderPane.alignment="CENTER">
        <tabs>
          <Tab text="Photo">
               <content>
                  <SplitPane dividerPositions="0.5">
                     <items>
                        <VBox AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.topAnchor="0.0">
                           <children>
                              <TableView fx:id="photoTable" editable="true" prefHeight="2000.0">
                                <columns>
                                    <TableColumn fx:id="photoNameColumn" prefWidth="75.0" text="Photo Name" />
                                  <TableColumn fx:id="countColumn" prefWidth="75.0" text="People Count" />
                                </columns>
                                 <columnResizePolicy>
                                    <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                                 </columnResizePolicy>
                              </TableView>
                              <ToolBar AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" VBox.vgrow="ALWAYS">
                                <items>
                                  <Button fx:id="addFileButton" mnemonicParsing="false" onAction="#handleAddFileButton" text="Add File" textAlignment="CENTER" />
                                    <Button fx:id="deleteSelectedPhotoButton" mnemonicParsing="false" onAction="#handleDeleteSelectedPhotoButton" text="Delete" textAlignment="CENTER" />
                                    <Button fx:id="scanButton" mnemonicParsing="false" onAction="#handleScanButton" text="Scan" textAlignment="CENTER" />
                                    <Button fx:id="scanAllButton" mnemonicParsing="false" onAction="#handleScanAllButton" text="Scan All" />
                                    <Button fx:id="detectAllButton" mnemonicParsing="false" onAction="#handleDetectAllButton" text="Detect All" />
                                </items>
                              </ToolBar>
                           </children>
                        </VBox>
                        <SplitPane dividerPositions="0.40148698884758366" orientation="VERTICAL">
                          <items>
                              <AnchorPane>
                                 <children>
                                    <HBox fx:id="photoImageViewParent" alignment="CENTER" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                                       <children>
                                          <ImageView fx:id="photoImageView" pickOnBounds="true" preserveRatio="true" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" />
                                       </children>
                                    </HBox>
                                 </children>
                              </AnchorPane>
                              <Accordion prefHeight="386.0" prefWidth="396.0">
                                <panes>
                                  <TitledPane animated="false" text="Photo Info">
                                    <content>
                                      <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                                             <children>
                                                <GridPane alignment="CENTER" gridLinesVisible="true" layoutX="14.0" layoutY="142.0" prefHeight="76.0" prefWidth="140.0" AnchorPane.rightAnchor="46.0">
                                                  <columnConstraints>
                                                    <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                                                    <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                                                  </columnConstraints>
                                                  <rowConstraints>
                                                    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                                                    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                                                    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                                                  </rowConstraints>
                                                   <children>
                                                      <Label text="File Name" />
                                                      <Label text="Label" GridPane.columnIndex="1" />
                                                      <Label text="People" GridPane.rowIndex="1" />
                                                      <Label text="Label" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                                                      <Label text="Selected People" GridPane.rowIndex="2" />
                                                      <ChoiceBox prefWidth="150.0" GridPane.columnIndex="1" GridPane.rowIndex="2" />
                                                   </children>
                                                   <padding>
                                                      <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                                                   </padding>
                                                </GridPane>
                                             </children>
                                          </AnchorPane>
                                    </content>
                                  </TitledPane>
                                  <TitledPane animated="false" text="Faces">
                                    <content>
                                      <AnchorPane>
                                             <children>
                                                <ToolBar AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                                                  <items>
                                                      <ComboBox fx:id="facesCombo" promptText="Please Scan This Photo First!" />
                                                    <Button fx:id="deleteFaceButton" mnemonicParsing="false" onAction="#handleDeleteFaceButton" text="Delete" />
                                                  </items>
                                                </ToolBar>
                                                <ImageView fx:id="faceImageView" fitHeight="200.0" fitWidth="180.0" pickOnBounds="true" preserveRatio="true" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" />
                                                <TextArea fx:id="idText" editable="false" layoutX="212.0" layoutY="68.0" prefHeight="99.0" prefWidth="135.0" />
                                                <TextField fx:id="idTextField" editable="false" layoutX="212.0" layoutY="183.0" />
                                             </children>
                                          </AnchorPane>
                                    </content>
                                  </TitledPane>
                                </panes>
                              </Accordion>
                          </items>
                        </SplitPane>
                     </items>
                  </SplitPane>
               </content>
          </Tab>
          <Tab text="People">
               <content>
                  <SplitPane dividerPositions="0.5" orientation="VERTICAL">
                    <items>
                        <VBox>
                           <children>
                              <ToolBar>
                                <items>
                                  <Button fx:id="addPeopleButton" mnemonicParsing="false" onAction="#handleAddPeople" text="Add People" />
                                </items>
                              </ToolBar>
                              <SplitPane dividerPositions="0.5">
                                <items>
                                    <TableView fx:id="humanTable">
                                      <columns>
                                        <TableColumn fx:id="humanNameColumn" prefWidth="75.0" text="名字" />
                                        <TableColumn fx:id="humanPhotoNumberColumn" prefWidth="75.0" text="Total Photo" />
                                      </columns>
                                       <columnResizePolicy>
                                          <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                                       </columnResizePolicy>
                                    </TableView>
                                    <TableView fx:id="humanPhotoTable">
                                      <columns>
                                        <TableColumn fx:id="humanPhotoNameColumn" prefWidth="75.0" text="Photo Name" />
                                        <TableColumn fx:id="humanPhotoLocationColumn" prefWidth="75.0" text="Photo Location" />
                                      </columns>
                                       <columnResizePolicy>
                                          <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                                       </columnResizePolicy>
                                    </TableView>
                                </items>
                              </SplitPane>
                           </children>
                        </VBox>
                        <SplitPane dividerPositions="0.29797979797979796" prefHeight="160.0" prefWidth="200.0">
                          <items>
                              <AnchorPane>
                                 <children>
                                    <HBox fx:id="humanPhotoImageViewParent" alignment="CENTER" prefHeight="265.0" prefWidth="234.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                                       <children>
                                          <ImageView fx:id="humanPhotoImageView" pickOnBounds="true" preserveRatio="true" />
                                       </children>
                                    </HBox>
                                 </children>
                              </AnchorPane>
                            <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="160.0" prefWidth="100.0" />
                          </items>
                        </SplitPane>
                    </items>
                  </SplitPane>
               </content>
          </Tab>
        </tabs>
      </TabPane>
   </center>
</BorderPane>
